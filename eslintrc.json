{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-jsx",
    "prettier",
    "prettier/react",
    "prettier/standard"
  ],
  "plugins": [
    "standard",
    "react",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error", {
        "semi": false,
        "arrowParens": "always",
        "jsxBracketSameLine": true,
        "singleQuote": true,
        "parser": "flow"
      }
    ]
  }
}
